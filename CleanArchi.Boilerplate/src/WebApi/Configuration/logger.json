{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Hangfire": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "Using": [],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId",
      "WithHangfireContext"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Hangfire": "Warning",
        "Microsoft": "Error",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Information"
      }
    },
    "Properties": {
      "Application": "FSH.WebApi"
    },
    "WriteTo": [
      {
        "Args": {
          "path": "Logs/logs.log",
          //"formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog",
          "rollingInterval": "Day",
          "restrictedToMinimumLevel": "Information",
          "retainedFileCountLimit": 5
        },
        "Name": "File"
      },
      //{
      //  "Name": "Seq", //out to 日志服务器(run at one server)
      //  "Args": {
      //    "serverUrl": "http://localhost:5341"
      //  }
      //},
      //{
      //  "Name": "Elasticsearch",//out to ES
      //  "Args": {
      //    "nodeUris": "http://localhost:9200;",
      //    "indexFormat": "CBAPI.WebApi-logs-{0:yyyy.MM}",
      //    "numberOfShards": 2,
      //    "numberOfReplicas": 1,
      //    "restrictedToMinimumLevel": "Information"
      //  }
      //}
    ]
  }
}